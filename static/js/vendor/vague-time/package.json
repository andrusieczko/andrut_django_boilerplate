{
    "name": "vague-time",
    "version": "0.5.2",
    "author": "Phil Booth <pmbooth@gmail.com>",
    "description": "A tiny library that formats precise time differences as a vague/fuzzy time, e.g. '3 months ago', 'just now' or 'in 2 hours'.",
    "contributors": [
        { "name": "Phil Booth", "email": "pmbooth@gmail.com" },
        { "name": "seth-cameo" },
        { "name": "Pepijn de Vos" }
    ],
    "main": "./src/vagueTime",
    "repository": {
        "type": "git",
        "url": "https://github.com/philbooth/vagueTime.js"
    },
    "keywords": [
        "time",
        "date",
        "fuzzy",
        "vague",
        "estimate",
        "ender"
    ],
    "ender": "noop",
    "jam": {
        "main": "src/vagueTime.js"
    },
    "devDependencies": {
        "jake": "0.5.x",
        "jshint": "1.0.x",
        "mocha": "1.7.x",
        "chai": "1.4.x",
        "uglify-js": "2.2.x"
    },
